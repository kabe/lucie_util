# example of new config file for setup-storage
#
# <type> <mountpoint> <size>   <fs type> <mount options> <misc options>
#
disk_config disk1 preserve_reinstall:8

primary  /             2G-20G     ext3   rw,errors=remount-ro
logical  swap          200-4G     swap   rw
logical  /var          2G-10G     ext3   rw         createopts="-m 5" tuneopts="-c 0 -i 0"
logical  /tmp          2G-10G     ext3   rw         createopts="-m 0" tuneopts="-c 0 -i 0"
logical  /data/local   1G-        ext3   rw         createopts="-m 1" tuneopts="-c 0 -i 0"
